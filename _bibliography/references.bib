	Date-Modified = {2015-03-02 22:35:55 +0000},
	Journal = {Deep Sea Research},
	Pages = {217--233},
	Title = {On the abyssal circulation of the world ocean II. An idealized model of the circulation pattern and amplitude in oceanic basins},
	Volume = {6},
	Year = {1959}}

